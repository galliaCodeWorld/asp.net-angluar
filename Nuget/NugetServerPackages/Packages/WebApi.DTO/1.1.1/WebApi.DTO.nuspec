<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>WebApi.DTO</id>
    <version>1.1.1</version>
    <title>WebApi.DTO</title>
    <authors>MultiPlatformChat</authors>
    <owners>MultiPlatformChat</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>This library is a Proxy between the Web Api Server and the Data Application Tier used by an application. It provides Data Transfer Objects, FlyWeight for httpclient, and classes to unify communications with the webapi server. Data tier must Map the DTOs to their local business domain objects.</description>
    <copyright>Copyright ©  2017</copyright>
    <dependencies>
      <group targetFramework=".NETPortable4.6-Profile151">
        <dependency id="Newtonsoft.Json" version="9.0.1" exclude="Build,Analyzers" />
        <dependency id="NofB.Org.Validation" version="1.0.0" exclude="Build,Analyzers" />
        <dependency id="System.ComponentModel.Annotations" version="4.3.0" exclude="Build,Analyzers" />
        <dependency id="System.Net.Http" version="4.3.1" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
</package>